<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="AvailabilityAlertSubscriptionRequest">
        <property name="subscriberIp" type="string" />
    </transfer>

    <transfer name="AvailabilityAlertSubscriber">
        <property name="hash" type="string" />
        <property name="key" type="string" />
        <property name="subscriberIp" type="string" />
        <property name="isActive" type="bool" />
        <property name="exported" type="bool" />
    </transfer>

    <transfer name="AvailabilityAlertCrossEngageSubscriberRegistrationResponse">
        <property name="subscriber" type="AvailabilityAlertSubscriber" />
        <property name="status" type="bool" />
    </transfer>

    <transfer name="AvailabilityAlertCrossEngageDispatchSubscriptionResponse">
        <property name="subscription" type="AvailabilityAlertSubscription" />
        <property name="status" type="bool" />
    </transfer>

</transfers>

