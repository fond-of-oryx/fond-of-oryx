{
  "name": "fond-of-oryx/credit-memo",
  "description": "Spryker Credit Memo Module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Jozsef Geng",
      "email": "gengjozsef86@gmail.com"
    },
    {
      "name": "Julian Hyatt",
      "email": "dev@julianzimmermann.dev"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-oryx/credit-memo-extension": "^1.1.0",
    "spryker/log": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "spryker/oms": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^10.0.0 || ^11.0.0",
    "spryker/payment": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "spryker/sales": "^8.0.0 || ^10.0.0 || ^11.0.0",
    "spryker/sequence-number": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "spryker/store": "^1.1.0",
    "spryker-eco/payone": "^4.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
