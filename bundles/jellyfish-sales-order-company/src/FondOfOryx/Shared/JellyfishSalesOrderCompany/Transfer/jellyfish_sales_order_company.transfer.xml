<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 https://static.spryker.com/transfer-01.xsd">
    <transfer name="JellyfishOrder">
        <property name="companyId" type="int"/>
        <property name="companyUuid" type="string"/>
        <property name="companyLocale" type="string"/>
        <property name="companyCurrency" type="string"/>
    </transfer>

    <transfer name="Company">
        <property name="idCompany" type="int"/>
        <property name="uuid" type="string"/>
        <property name="fkLocale" type="int"/>
        <property name="fkCurrency" type="int"/>
    </transfer>

    <transfer name="Locale">
        <property name="name" type="string"/>
    </transfer>

    <transfer name="Currency">
        <property name="code" type="string"/>
    </transfer>
</transfers>
