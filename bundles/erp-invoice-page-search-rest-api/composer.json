{
  "name": "fond-of-oryx/erp-invoice-page-search-rest-api",
  "description": "ERP Invoice Page Search Rest Api module",
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Hyatt",
      "email": "info@julianzimmermann.dev"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-oryx/erp-invoice-page-search": "^2.0.0 || ^3.0.0",
    "spryker/glossary-storage": "^1.5.0",
    "spryker/glue-application": "^1.0.0",
    "spryker/glue-application-extension": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "tests/Generated/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
