<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="JellyfishOrder">
        <property name="discounts" type="JellyfishOrderDiscount[]" singular="discount"/>
    </transfer>

    <transfer name="JellyfishOrderAddress">
        <property name="firstName" type="string"/>
        <property name="lastName" type="string"/>
        <property name="name1" type="string"/>
        <property name="name2" type="string"/>
    </transfer>

    <transfer name="JellyfishOrderDiscount">
        <property name="idSalesOrderItem" type="int"/>
        <property name="code" type="string"/>
        <property name="name" type="string"/>
        <property name="sumAmount" type="int"/>
        <property name="quantity" type="int"/>
    </transfer>
</transfers>
