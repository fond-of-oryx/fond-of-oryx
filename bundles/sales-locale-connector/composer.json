{
  "name": "fond-of-oryx/sales-locale-connector",
  "description": "Sales locale connector module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "spryker/locale": "^2.2.0 || ^3.0.0",
    "spryker/sales": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^10.0.0 || ^11.0.0",
    "spryker/sales-extension": "^1.3.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "fond-of-codeception/spryker": "dev-master",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.14",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
