{
  "name": "fond-of-oryx/companies-rest-api",
  "description": "Companies Rest Api",
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Hyatt",
      "email": "dev@jzit.de"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-oryx/companies-rest-api-permission": "^1.0.0",
    "fond-of-oryx/company-deleter": "^1.0.0",
    "spryker/glue-application": "*"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^2.0.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
