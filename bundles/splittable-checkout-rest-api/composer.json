{
  "name": "fond-of-oryx/splittable-checkout-rest-api",
  "description": "Splittable Checkout Rest Api Module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-oryx/splittable-checkout": "^1.0.0 || ^2.0.0",
    "fond-of-oryx/splittable-checkout-rest-api-extension": "^1.0.0 || ^2.0.0",
    "fond-of-oryx/splittable-totals": "^1.0.0 || ^2.0.0",
    "spryker/cart": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "spryker/glossary-storage": "^1.0.0",
    "spryker/glue-application": "^1.0.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/quote": "^2.0.0",
    "spryker/zed-request": "^3.3.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "*",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/",
      "Orm\\": "src/Orm/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
