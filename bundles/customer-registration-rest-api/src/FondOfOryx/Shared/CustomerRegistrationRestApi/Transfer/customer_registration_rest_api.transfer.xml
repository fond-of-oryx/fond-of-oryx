<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="RestCustomerRegistrationRequestAttributes">
        <property name="email" type="string"/>
        <property name="acceptGdpr" type="bool"/>
        <property name="callbackUrl" type="string"/>
    </transfer>

    <transfer name="RestCustomerRegistrationResponse">
        <property name="email" type="string" />
    </transfer>

    <transfer name="CustomerRegistrationKnownCustomerResponse">
        <property name="message" type="string"/>
    </transfer>

    <transfer name="HandleKnownCustomer">
        <property name="customer" type="Customer"/>
        <property name="oneTimePasswordAttributes" type="OneTimePasswordAttributes"/>
    </transfer>

    <transfer name="OneTimePasswordAttributes">
        <property name="callbackUl" type="string" />
    </transfer>
</transfers>
