<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="PaymentEpcQrCodeRequest">
        <property name="serviceTag" type="string" />
        <property name="version" type="string" />
        <property name="encoding" type="int" />
        <property name="type" type="string" />
        <property name="bic" type="string" />
        <property name="iban" type="string" />
        <property name="receiverName" type="string" />
        <property name="iban" type="string" />
        <property name="amount" type="string" />
        <property name="purpose" type="string" />
        <property name="reference" type="string" />
        <property name="usage" type="string" />
