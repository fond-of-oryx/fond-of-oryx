    <transfer name="PaymentEpcQrCodeResponse">
        <property name="isSuccess" type="boolean" />
        <property name="data" type="string" />
    </transfer>

    <transfer name="Order">
        <property name="prepaymentEpcQrData" type="string" />
    </transfer>
