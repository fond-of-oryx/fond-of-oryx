<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 https://static.spryker.com/transfer-01.xsd">

    <transfer name="RestRepresentationOfSalesAttributes">
        <property name="uuid" type="string"/>
        <property name="referenceDistributor" restRequestParameter="required" type="string" />
        <property name="referenceRepresentation" restRequestParameter="required" type="string" />
        <property name="referenceOriginator" restRequestParameter="required" type="string" />
        <property name="startDate" type="int" />
        <property name="endDate" type="int" />
    </transfer>

    <transfer name="RestRepresentationOfSalesRequest">
        <property name="attributes" type="RestRepresentationOfSalesAttributes"/>
    </transfer>

    <transfer name="RestRepresentationOfSalesResponse">
        <property name="request" type="RestRepresentationOfSalesRequest"/>
        <property name="representation" type="RepresentationOfSales"/>
    </transfer>

    <transfer name="RepresentationOfSales" />
    <transfer name="RepresentationOfSalesCollection" />
    <transfer name="RepresentationOfSalesCommand" />
    <transfer name="RepresentationOfSalesFilter" />
</transfers>
