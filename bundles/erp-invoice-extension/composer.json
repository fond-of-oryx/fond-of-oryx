{
  "name": "fond-of-oryx/erp-invoice-extension",
  "description": "ERP Invoice Extension Module",
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Hyatt",
      "email": "info@julianzimmermann.dev"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^2.0.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
