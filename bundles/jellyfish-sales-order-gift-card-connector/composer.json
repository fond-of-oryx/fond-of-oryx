    "spryker-sdk/phpstan-spryker": "*",
    "spryker/gift-card": "*",
    "spryker/payment": "*",
    "spryker/sales": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
