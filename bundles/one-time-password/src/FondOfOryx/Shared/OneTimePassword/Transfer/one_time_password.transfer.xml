<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="OneTimePasswordResponse">
        <property name="isSuccess" type="bool"/>
        <property name="customerTransfer" type="Customer"/>
        <property name="oneTimePasswordPlain" type="string"/>
        <property name="loginLink" type="string"/>
    </transfer>

    <transfer name="OneTimePasswordAttributes">
        <property name="email" type="string"/>
        <property name="orderReference" type="string"/>
        <property name="locale" type="Locale"/>
    </transfer>

    <transfer name="Mail">
        <property name="oneTimePasswordLoginLink" type="string"/>
    </transfer>

    <transfer name="Customer">
        <property name="email" type="string"/>
        <property name="newPassword" type="string"/>
    </transfer>

    <transfer name="CustomerResponse">
        <property name="isSuccess" type="bool"/>
        <property name="customerTransfer" type="Customer"/>
    </transfer>

    <transfer name="OauthResponse">
        <property name="isValid" type="bool"/>
        <property name="getAccessToken" type="string"/>
        <property name="customerReference" type="string"/>
    </transfer>

    <transfer name="Order">
        <property name="orderReference" type="string"/>
    </transfer>
</transfers>
