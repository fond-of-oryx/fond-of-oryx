{
  "name": "fond-of-oryx/company-business-unit-search-rest-api",
  "description": "Company business unit search rest api bundle.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Julian Hyatt",
      "email": "dev@jzit.de"
    }
  ],
  "require": {
    "php": ">=8.2",
    "fond-of-oryx/company-business-unit-search-rest-api-extension": "^1.0.0",
    "spryker/company": "^1.0.0",
    "spryker/company-user": "^2.0.0",
    "spryker/company-business-unit": "^2.0.0",
    "spryker/glue-application": "^1.0.0",
    "spryker/glossary-storage": "^1.5.0",
    "spryker/uuid-behavior": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "*",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/",
      "Orm\\": "src/Orm/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
