{
  "name": "fond-of-oryx/product-list-search-rest-api",
  "description": "Product list search rest api bundle.",
  "license": "MIT",
  "authors": [
    {
      "name": "Jozsef Geng",
      "email": "gengjozsef86@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "spryker/product-list": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^2.0.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
