{
  "name": "fond-of-oryx/gift-card-proportional-value",
  "description": "GiftCardProportionalValue module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Hyatt",
      "email": "info@julianzimmermann.dev"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fond-of/payone"
    }
  ],
  "require": {
    "php": ">=7.3",
    "fond-of-oryx/gift-card-proportional-value-extension": "^1.0.0",
    "spryker/gift-card-balance": "^1.0.0",
    "spryker/sales": "^8.0.0 || ^10.0.0 || ^11.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  }
}
