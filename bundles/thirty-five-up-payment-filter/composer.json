{
  "name": "fond-of-oryx/thirty-five-up-payment-filter",
  "description": "Disable a payment method for selected products.",
  "license": "MIT",
  "authors": [
    {
      "name": "FOND OF GmbH",
      "email": "spryker@fondof.de"
    }
  ],
  "require": {
    "php": ">=7.3",
    "spryker/payment": "^4.3.0 || ^5.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/"
    }
  }
}
