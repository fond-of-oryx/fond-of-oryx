  "require": {
    "php": ">=7.3",
    "fond-of-oryx/order-budget": "^1.0.0",
    "spryker/checkout": "^6.0.0",
    "spryker/checkout-extension": "^1.2.0",
    "spryker/company-business-unit-extension": "^1.0.0",
    "spryker/quote-extension": "^1.2.0",
    "spryker/permission": "^1.0.0",
    "spryker/permission-extension": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
