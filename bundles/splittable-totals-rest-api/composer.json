{
  "name": "fond-of-oryx/splittable-totals-rest-api",
  "description": "Splittable Totals Rest Api Module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.3",
    "fond-of-oryx/splittable-totals": "^1.0.0",
    "fond-of-oryx/splittable-totals-rest-api-extension": "^1.0.0",
    "spryker/quote": "^2.0.0",
    "spryker/zed-request": "^3.3.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/"
    }
  }
}
