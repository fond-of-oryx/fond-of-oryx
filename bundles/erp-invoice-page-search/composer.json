{
  "name": "fond-of-oryx/erp-invoice-page-search",
  "description": "ERP Invoice Page Search module",
  "license": "MIT",
  "authors": [
    {
      "name": "Julian Hyatt",
      "email": "info@julianzimmermann.dev"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-oryx/erp-invoice": "^2.1.0 || ^3.0.0",
    "fond-of-oryx/erp-invoice-permission": "^1.0.0 || ^2.0.0",
    "spryker/customer": "^7.0.0",
    "spryker/event-behavior": "^1.0.0",
    "spryker/search": "^8.0.0",
    "spryker/search-elasticsearch": "^1.0.0",
    "spryker/synchronization-extension": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^2.0.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfOryx\\": "src/FondOfOryx/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfOryx\\": "tests/FondOfOryx/",
      "Generated\\": "src/Generated/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
